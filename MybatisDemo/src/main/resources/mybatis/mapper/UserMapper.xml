<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.joey.mybatisdemo.dao.UserDao" >

    <select id="get" resultType="com.joey.mybatisdemo.entity.User"  parameterType="Long">
        SELECT
        id,username,password
        FROM t_user
        WHERE
        id=#{id}
    </select>


</mapper>